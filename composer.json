        "FastRoute",
        "Aura.Di",
        "dependency injection",
        "DI container",
        "lightweight"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "~7.0",
        "zendframework/zend-diactoros": "~1.3",
        "nikic/fast-route": "~1.0",
        "aura/di": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Upscale\\HttpServerEngine\\": "src/"
        }
    }
