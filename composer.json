{
    "name": "upscale/http-server-engine",
    "description": "Wiring of Zend Diactoros, FastRoute, and Aura.Di for RESTful API",
    "type": "library",
    "version": "2.0.1",
    "keywords": [
        "HTTP",
        "REST",
        "RESTful",
        "API",
        "PSR-7",
        "Diactoros",
        "routing",
        "router",
        "FastRoute",
        "dependency injection",
        "DI",
        "Aura",
        "lightweight"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "~5.6 || ~7.0",
        "zendframework/zend-diactoros": "~1.3",
        "nikic/fast-route": "~1.0",
        "aura/di": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "Upscale\\HttpServerEngine\\": "src/"
        }
    }
}
